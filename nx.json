{
  "npmScope": "homeofthings",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "hot-express": {
      "tags": ["scope:backend"]
    },
    "hot-gateway": {
      "tags": ["scope:backend"]
    },
    "nestjs-config": {
      "tags": ["scope:backend"]
    },
    "nestjs-logger": {
      "tags": ["scope:backend"]
    },
    "nestjs-sqlite3": {
      "tags": ["scope:backend"]
    },
    "nestjs-utils": {
      "tags": ["scope:backend"]
    }
  },
  "workspaceLayout": {
    "appsDir": "projects",
    "libsDir": "projects"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
