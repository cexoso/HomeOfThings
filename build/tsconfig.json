{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "types": ["node"],
    "rootDir": ".",
    "removeComments": false,
    "preserveConstEnums": true,
    "allowJs": true,
    "esModuleInterop": false,
    "noImplicitAny": false,
    "typeRoots": ["../node_modules/@types", "ts/typings"]
  }
}
