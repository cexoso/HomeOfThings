{
  "version": 2,
  "projects": {
    "hot-cli": {
      "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
      "root": "projects/node/apps/hot-cli",
      "sourceRoot": "projects/node/apps/hot-cli/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/node/apps/hot-cli",
            "main": "projects/node/apps/hot-cli/src/main.ts",
            "tsConfig": "projects/node/apps/hot-cli/tsconfig.app.json",
            "assets": ["projects/node/apps/hot-cli/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "projects/node/apps/hot-cli/src/environments/environment.ts",
                  "with": "projects/node/apps/hot-cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "hot-cli:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["projects/node/apps/hot-cli/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/node/apps/hot-cli"],
          "options": {
            "jestConfig": "projects/node/apps/hot-cli/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:backend"]
    },
    "hot-gateway": {
      "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
      "root": "projects/node/apps/hot-gateway",
      "sourceRoot": "projects/node/apps/hot-gateway/src",
      "projectType": "application",
      "prefix": "hot",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx run hot-gateway:build-main", "nx run hot-gateway:build-post"],
            "parallel": false
          }
        },
        "build-main": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/node/apps/hot-gateway",
            "main": "projects/node/apps/hot-gateway/src/main.ts",
            "tsConfig": "projects/node/apps/hot-gateway/tsconfig.app.json",
            "assets": ["projects/node/apps/hot-gateway/src/assets"],
            "webpackConfig": "projects/node/apps/hot-gateway/custom-webpack.config.js",
            "tsPlugins": [
              {
                "name": "@nestjs/swagger/plugin",
                "options": {
                  "dtoFileNameSuffix": [".dto.ts", ".entity.ts"]
                }
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "projects/node/apps/hot-gateway/src/environments/environment.ts",
                  "with": "projects/node/apps/hot-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "build-post": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["node ./dist/projects/node/apps/hot-gateway/main.js export-api ./tmp/hot-openapi.json"],
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "hot-gateway:build-main"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/node/apps/hot-gateway/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/node/apps/hot-gateway"],
          "options": {
            "jestConfig": "projects/node/apps/hot-gateway/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:backend"]
    },
    "hot-server": {
