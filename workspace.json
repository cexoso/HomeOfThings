{
  "version": 2,
  "projects": {
    "config": {
      "root": "projects/node/libs/config",
      "sourceRoot": "projects/node/libs/config/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/node/libs/config/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/node/libs/config"],
          "options": {
            "jestConfig": "projects/node/libs/config/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/node/libs/config",
            "tsConfig": "projects/node/libs/config/tsconfig.lib.json",
            "packageJson": "projects/node/libs/config/package.json",
            "main": "projects/node/libs/config/src/index.ts",
            "assets": ["projects/node/libs/config/*.md"]
          }
        }
      }
    },
    "api-gateway": {
      "root": "projects/node/apps/api-gateway",
      "sourceRoot": "projects/node/apps/api-gateway/src",
      "projectType": "application",
      "prefix": "hot",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/node/apps/api-gateway",
            "main": "projects/node/apps/api-gateway/src/main.ts",
            "tsConfig": "projects/node/apps/api-gateway/tsconfig.app.json",
            "assets": ["projects/node/apps/api-gateway/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "projects/node/apps/api-gateway/src/environments/environment.ts",
                  "with": "projects/node/apps/api-gateway/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-gateway:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/node/apps/api-gateway/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/node/apps/api-gateway"],
          "options": {
            "jestConfig": "projects/node/apps/api-gateway/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "logger": {
      "root": "projects/node/libs/logger",
      "sourceRoot": "projects/node/libs/logger/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/node/libs/logger/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/node/libs/logger"],
          "options": {
            "jestConfig": "projects/node/libs/logger/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/node/libs/logger",
            "tsConfig": "projects/node/libs/logger/tsconfig.lib.json",
            "packageJson": "projects/node/libs/logger/package.json",
            "main": "projects/node/libs/logger/src/index.ts",
            "assets": ["projects/node/libs/logger/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nstudio/xplat"
  },
  "defaultProject": "api-gateway"
}
